{
  "name": "caferoam",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prebuild:pass0": "tsx scripts/prebuild/data-pipeline/pass0-seed.ts",
    "prebuild:pass1": "tsx scripts/prebuild/data-pipeline/pass1-verify.ts",
    "prebuild:pass2": "tsx scripts/prebuild/data-pipeline/pass2-scrape.ts",
    "prebuild:pass3a": "tsx --env-file=.env.local scripts/prebuild/data-pipeline/pass3a-taxonomy-seed.ts",
    "prebuild:pass3b": "tsx --env-file=.env.local scripts/prebuild/data-pipeline/pass3b-enrich.ts",
    "prebuild:pass3c": "tsx scripts/prebuild/data-pipeline/pass3c-postprocess.ts",
    "prebuild:pass4": "tsx --env-file=.env.local scripts/prebuild/data-pipeline/pass4-embed.ts",
    "prebuild:pass5": "tsx --env-file=.env.local scripts/prebuild/data-pipeline/pass5-search-test.ts",
    "prebuild:all": "pnpm prebuild:pass0 && pnpm prebuild:pass1 && pnpm prebuild:pass2 && echo 'Run pass3a, curate taxonomy, then pass3b → pass3c → pass4 → pass5 manually'",
    "backend:dev": "cd backend && uv run uvicorn main:app --reload --port 8000",
    "backend:test": "cd backend && uv run python -m pytest -v"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.97.0",
    "apify-client": "^2.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fuzzball": "^2.2.3",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "openai": "^6.22.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
